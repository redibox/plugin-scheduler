{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "import/no-unresolved": 0
  },
  "globals": {
    "describe": true,
    "RediBox": true,
    "RediBoxCluster": true,
    "HOOK_NAME": true,
    "beforeEach": true,
    "Hook": true,
    "HookCluster": true,
    "it": true,
    "before": true,
    "after": true
  }
}
